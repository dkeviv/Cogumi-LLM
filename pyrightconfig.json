{
  "include": [
    "src",
    "scripts",
    "tests"
  ],
  "exclude": [
    "**/__pycache__",
    "**/.pytest_cache",
    "**/.venv",
    "**/venv",
    "**/.ipynb_checkpoints",
    "**/node_modules"
  ],
  "ignore": [
    "notebooks/**/*.ipynb"
  ],
  "reportMissingImports": "none",
  "reportMissingTypeStubs": false,
  "pythonVersion": "3.10",
  "pythonPlatform": "All"
}
